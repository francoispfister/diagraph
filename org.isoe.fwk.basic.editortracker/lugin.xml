<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="org.isoe.fwk.basic.editortracker.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/sample.gif"
               class="org.isoe.fwk.basic.editortracker.actions.SampleAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="org.isoe.fwk.basic.editortracker.actions.SampleAction">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="Sample Category"
            id="org.isoe.fwk.basic.editortracker.commands.category">
      </category>
      <command
            name="Sample Command"
            categoryId="org.isoe.fwk.basic.editortracker.commands.category"
            id="org.isoe.fwk.basic.editortracker.commands.sampleCommand">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="org.isoe.fwk.basic.editortracker.commands.sampleCommand"
            class="org.isoe.fwk.basic.editortracker.handlers.SampleHandler">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.isoe.fwk.basic.editortracker.commands.sampleCommand"
            contextId="org.eclipse.ui.contexts.window"
            sequence="M1+6"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="org.isoe.fwk.basic.editortracker.menus.sampleMenu">
            <command
                  commandId="org.isoe.fwk.basic.editortracker.commands.sampleCommand"
                  mnemonic="S"
                  id="org.isoe.fwk.basic.editortracker.menus.sampleCommand">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.isoe.fwk.basic.editortracker.toolbars.sampleToolbar">
            <command
                  commandId="org.isoe.fwk.basic.editortracker.commands.sampleCommand"
                  icon="icons/sample.gif"
                  tooltip="Say hello world"
                  id="org.isoe.fwk.basic.editortracker.toolbars.sampleCommand">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            name="Sample Page"
            nameFilter="*.*"
            class="org.isoe.fwk.basic.editortracker.properties.SamplePropertyPage"
            id="org.isoe.fwk.basic.editortracker.properties.samplePropertyPage">
         <enabledWhen>
            <instanceof
                  value="org.eclipse.core.resources.IFile">
            </instanceof>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample XML Editor"
            extensions="xml"
            icon="icons/sample.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="org.isoe.fwk.basic.editortracker.editors.XMLEditor"
            id="org.isoe.fwk.basic.editortracker.editors.XMLEditor">
      </editor>
      <editor
            name="Sample Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="org.isoe.fwk.basic.editortracker.editors.MultiPageEditorContributor"
            class="org.isoe.fwk.basic.editortracker.editors.MultiPageEditor"
            id="org.isoe.fwk.basic.editortracker.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="org.isoe.fwk.basic.editortracker.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="org.isoe.fwk.basic.editortracker.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="org.isoe.fwk.basic.editortracker.popup.actions.NewAction"
               menubarPath="org.isoe.fwk.basic.editortracker.menu1/group1"
               enablesFor="1"
               id="org.isoe.fwk.basic.editortracker.newAction">
         </action>
      </objectContribution>
      <objectContribution
            adaptable="true"
            objectClass="org.eclipse.core.resources.IProject"
            nameFilter="*"
            id="org.isoe.fwk.basic.editortracker.contribution1">
         <action
               label="Add/Remove Sample Nature"
               class="org.isoe.fwk.basic.editortracker.builder.ToggleNatureAction"
               menubarPath="additions"
               enablesFor="+"
               id="org.isoe.fwk.basic.editortracker.addRemoveNatureAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            name="My Common Navigator"
            icon="icons/sample.gif"
            class="org.eclipse.ui.navigator.CommonNavigator"
            id="com.example.test">
      </view>
      <category
            name="Sample Category"
            id="org.isoe.fwk.basic.editortracker">
      </category>
      <view
            name="Sample View"
            icon="icons/sample.gif"
            category="org.isoe.fwk.basic.editortracker"
            class="org.isoe.fwk.basic.editortracker.views.SampleView"
            id="org.isoe.fwk.basic.editortracker.views.SampleView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerActionBinding
            viewerId="com.example.test">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
      <viewerContentBinding
            viewerId="com.example.test">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.ui.resourcePerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.navigator.ProjectExplorer"
               relationship="stack"
               id="com.example.test">
         </view>
      </perspectiveExtension>
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="org.isoe.fwk.basic.editortracker.views.SampleView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="org.isoe.fwk.basic.editortracker">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="org.isoe.fwk.basic.editortracker"
            class="org.isoe.fwk.basic.editortracker.wizards.SampleNewWizard"
            id="org.isoe.fwk.basic.editortracker.wizards.SampleNewWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="Sample Preferences"
            class="org.isoe.fwk.basic.editortracker.preferences.SamplePreferencePage"
            id="org.isoe.fwk.basic.editortracker.preferences.SamplePreferencePage">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.isoe.fwk.basic.editortracker.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            lightweight="true"
            location="BOTTOM_RIGHT"
            adaptable="true"
            label="Resource Decorator"
            icon="icons/sample_decorator.gif"
            state="true"
            id="org.isoe.fwk.basic.editortracker.decorator">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <or>
                  <objectClass
                        name="org.eclipse.core.resources.IProject">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
               </or>
            </and>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            name="Sample File Import"
            id="org.isoe.fwk.basic.editortracker.importWizards.sampleCategory">
      </category>
      <wizard
            name="Import File"
            icon="icons/sample.gif"
            category="org.isoe.fwk.basic.editortracker.importWizards.sampleCategory"
            class="org.isoe.fwk.basic.editortracker.importWizards.ImportWizard"
            id="org.isoe.fwk.basic.editortracker.importWizards.ImportWizard">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            content="intro/sample.xml"
            configId="org.eclipse.ui.intro.universalConfig">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="Release Engineering"
            icon="icons/releng_gears.gif"
            class="org.isoe.fwk.basic.editortracker.perspectives.RelEngPerspective"
            id="org.isoe.fwk.basic.editortracker.perspectives.RelEngPerspective">
      </perspective>
   </extension>
   <extension
         id="sampleBuilder"
         name="Sample Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="org.isoe.fwk.basic.editortracker.builder.SampleBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="sampleNature"
         name="Sample Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.isoe.fwk.basic.editortracker.builder.SampleNature">
         </run>
      </runtime>
      <builder
            id="org.isoe.fwk.basic.editortracker.sampleBuilder">
      </builder>
   </extension>
   <extension
         id="xmlProblem"
         name="XML Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="org.isoe.fwk.basic.editortracker.splashHandlers.InteractiveSplashHandler"
            id="org.isoe.fwk.basic.editortracker.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            splashId="org.isoe.fwk.basic.editortracker.splashHandlers.interactive"
            productId="org.eclipse.sdk.ide">
      </splashHandlerProductBinding>
   </extension>

</plugin>
